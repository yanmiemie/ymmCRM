OAAA,GAAA,CAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,KAAA,KAAA,CAAA,KAAA,OAAA,CAAA;AACA,aAAA,KAAA,CAAA,MAAA,CAAA,KAAA,OAAA,EAAA,CAAA;AACA;AACA;AAtBA;AAhBA,CAAA","sourcesContent":["<template>\nsss\n    <div id=\"app\">\n  <div class=\"container\">\n    <v-list class=\"list\">\n      <v-list-tile\n           v-for=\"(item, index) in items\"\n           :key=\"`item-${index}`\"\n           draggable\n           class=\"list__item\"\n           @dragstart=\"dragStart($event, index)\"\n           @dragend=\"dragEnd\">\n        <v-list-tile-title>\n          {{ item }}\n        </v-list-tile-title>\n      </v-list-tile>\n    </v-list>\n    <main\n          :class=\"{ layered : isDraggedOver }\"\n          @dragover=\"dragOver\"\n          @dragleave=\"dragLeave\"\n          @drop=\"drop\">\n      <div\n            v-for=\"(drop, index) in dropped\"\n            :key=\"`d-${index}`\"\n            class=\"bold\">\n        {{ drop }}\n      </div>\n    </main>\n  </div>\n</div>\n\n</template>\n <style>\n \n \n </style>\n\n<script>\nVue.use(Vuetify)\n\nnew Vue({\n  el : '#app',\n  data () {\n    return {\n      isDraggedOver : false,\n      items : [\n        'Element 1',\n        'Element 2',\n        'Element 3',\n        'Element 4',\n        'Element 5',\n      ],\n      dropped : [],\n      dragged : null,\n    }\n  },\n  methods : {\n    dragStart (event, index) {\n      this.dragged = index\n      event.target.style.opacity = 0.5\n    },\n    dragOver (event) {\n      event.preventDefault()\n      this.isDraggedOver = true\n    },\n    dragLeave (event) {\n      event.preventDefault()\n      this.isDraggedOver = false\n    },\n    dragEnd (event) {\n      event.target.style.opacity = 1\n      this.dragged = null\n    },\n    drop (event) {\n      if (this.dragged > -1) {\n        this.dropped.push(this.items[this.dragged])\n        this.items.splice(this.dragged, 1)\n      }\n    },\n  },\n})\n</script>\n"],"sourceRoot":"src/components"}]}
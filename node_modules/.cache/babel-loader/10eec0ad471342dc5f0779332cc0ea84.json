AA;AANA,SAAA;AAQA,OAXA;AAYA,WAAA,SAAA,GAAA,UAAA;AACA,KAjBA;AAmBA,IAAA,WAnBA,yBAmBA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,CAAA,CAAA;AACA,KAtBA;AAwBA,IAAA,iBAxBA,6BAwBA,QAxBA,EAwBA,KAxBA,EAwBA;AACA,WAAA,eAAA,GAAA,QAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KA3BA,CA6BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AArCA,GAXA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,IAAA,yBAAA,CAAA,MAAA,GAAA,EAAA,CAAA,OAAA,EAAA,KAAA,YAAA;AACA,GApDA;AAqDA,EAAA,aArDA,2BAqDA;AACA,IAAA,yBAAA,CAAA,MAAA,GAAA,GAAA,CAAA,OAAA,EAAA,KAAA,YAAA;AACA;AAvDA,CAAA","sourcesContent":["<template>\n  <div class=\"list row\">\n    <div class=\"col-md-8\">\n      <!-- <h4> 列表 </h4> -->\n      <a class=\"text-3xl py-2 \"> 目前收費設定 </a> \n      <v-btn flat class=\"mx-2\">\n        <router-link to=\"/PMSAdd\">新增項目 </router-link>\n      </v-btn> \n      \n      <ul class=\"list-group\">\n        <li\n          class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(tutorial, index) in tutorials\"\n          :key=\"index\"\n          @click=\"setActiveTutorial(tutorial, index)\"\n        >\n          {{ tutorial.name }} , \n          {{ tutorial.note }} , \n          {{ tutorial.price }} ,   \n          {{ tutorial.type }} ,  \n          {{ tutorial.status }} ,  \n        </li>\n      </ul>\n\n      \n \n\n      <!-- <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllTutorials\">\n        千萬不要按\n      </button> -->\n    </div>\n    <div class=\"col-md-4\">\n      <div v-if=\"currentTutorial\">\n        <tutorial-details\n          :tutorial=\"currentTutorial\"\n          @refreshList=\"refreshList\"\n        />\n      </div>\n      <div v-else>\n        <br />\n        <p>請點選項目進行 查核</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PayMentSettingDataService from \"../services/PayMentSettingDataService\";\nimport TutorialDetails from \"./PMSMdf\"; // 連接至 Mdf 的部分\n\nexport default {\n  name: \"PayMentSetting-list\",\n  components: { TutorialDetails },\n  data() {\n    return {\n      Moneyflow:['現金','線上刷','抵用券','其他'],\n      tutorials: [],\n      currentTutorial: null,\n      currentIndex: -1\n    };\n  },\n  methods: {\n      onDataChange(items) {\n        let _tutorials = [];\n\n        items.forEach((item) => {\n          let key = item.key;\n          let data = item.val();\n          _tutorials.push({\n            key: key, \n            name    : data.name,\n            note    : data.note,  \n            price   : data.price,\n            status  : data.status,\n            type    : data.type, \n          });\n        }); \n        this.tutorials = _tutorials;\n      },\n\n      refreshList() {\n        this.currentTutorial = null;\n        this.currentIndex = -1;\n      },\n\n      setActiveTutorial(tutorial, index) {\n        this.currentTutorial = tutorial;\n        this.currentIndex = index;\n      },\n\n      // removeAllTutorials() {\n      //   WordDataService.deleteAll()\n      //     .then(() => {\n      //       this.refreshList();\n      //     })\n      //     .catch((e) => {\n      //       console.log(e);\n      //     });\n      // },\n  },\n  mounted() {\n    PayMentSettingDataService.getAll().on(\"value\", this.onDataChange);\n  },\n  beforeDestroy() {\n    PayMentSettingDataService.getAll().off(\"value\", this.onDataChange);\n  }\n};\n</script>\n\n<style>\n.list {\n  text-align: left;\n  max-width: 950px;\n  margin: auto;\n}\n</style>\n \n  "],"sourceRoot":"src/components"}]}
AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,eAAA,CAAA,IADA;AAEA,QAAA,IAAA,EAAA,KAAA,eAAA,CAAA,IAFA;AAGA,QAAA,IAAA,EAAA,KAAA,eAAA,CAAA,IAHA;AAIA,QAAA,MAAA,EAAA,KAAA,eAAA,CAAA;AAJA,OAAA;AAOA,MAAA,mBAAA,CAAA,MAAA,CAAA,KAAA,eAAA,CAAA,GAAA,EAAA,IAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,OAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA,KA7BA;AA+BA,IAAA,cA/BA,4BA+BA;AAAA;;AACA,MAAA,mBAAA,CAAA,MAAA,CAAA,KAAA,eAAA,CAAA,GAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,aAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OANA;AAOA;AAvCA,GAhBA;AAyDA,EAAA,OAzDA,qBAyDA;AACA,SAAA,OAAA,GAAA,EAAA;AACA,SAAA,eAAA,qBAAA,KAAA,QAAA;AACA;AA5DA,CAAA","sourcesContent":["<template>\n  <div v-if=\"currentTutorial\" class=\"edit-form\">\n    <h4>  修改內容  </h4>\n    <form> \n                  <v-text-field   \n                    label=\"請填寫 名稱\"\n                    solo\n                    clearable\n                    v-model=\"currentTutorial.name\"\n                  ></v-text-field>  \n\n                  <v-textarea\n                    solo\n                    name=\"input-7-4\"\n                    label=\"請註記 用途\"\n                    clearable\n                    v-model=\"currentTutorial.note\"\n                  ></v-textarea> \n \n                  <v-select\n                    v-model=\"currentTutorial.type\"\n                    :items=\"Moneyflow\"\n                    label=\"請設定 ...使用類別\"\n                    multiple\n                    chips \n                    persistent-hint\n                ></v-select> \n\n      <div class=\"form-group\">\n        <label><strong>狀態:</strong></label>\n        {{ currentTutorial.status ? \"true\" : \"false\" }}\n      </div>\n \n    </form>\n\n    <button\n      class=\"border-2 border-solid px-2 mr-2\"\n      v-if=\"currentTutorial.status != true \"\n      @click=\"updatePublished(false)\"\n    >\n      停用設定\n    </button>\n    <button\n      v-else\n      class=\"border-2 border-solid px-2 mr-2\"\n      @click=\"updatePublished(true)\"\n    >\n      儲存\n    </button>\n\n    <button class=\"border-2 border-solid px-2 mr-2\" @click=\"deleteTutorial\">\n      刪除\n    </button>\n\n    <button type=\"submit\" class=\"border-2 border-solid px-2\" @click=\"updateTutorial\">\n      更新項目\n    </button>\n    <p>{{ message }}</p>\n  </div>\n\n  <div v-else>\n    <br />\n    <p> 請點選 消費項目...</p>\n  </div>\n</template>\n\n<script>\nimport TutorialDataService from \"../services/PayMentNoteDataService\";\n\nexport default {\n  name: \"tutorial\",\n  props: [\"tutorial\"],\n  data() {\n    return {\n      currentTutorial: null,\n      message: \"\",\n      Moneyflow:['現金','線上刷','抵用券','其他'],\n    };\n  },\n  watch: {\n    tutorial: function(tutorial) {\n      this.currentTutorial = { ...tutorial };\n      this.message = \"\";\n    },\n  },\n  methods: {\n    updatePublished(status) {\n      TutorialDataService.update(this.currentTutorial.key, {\n        published: status,\n      })\n        .then(() => {\n          this.currentTutorial.published = status;\n          this.message = \"停用這項設定 (功能未完成)\";\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    updateTutorial() {\n      const data = {\n        name: this.currentTutorial.name,\n        note: this.currentTutorial.note,\n        type: this.currentTutorial.type,\n        status: this.currentTutorial.note,\n      };\n\n      TutorialDataService.update(this.currentTutorial.key, data)\n        .then(() => {\n          this.message = \"更新成功!\";\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    deleteTutorial() {\n      TutorialDataService.delete(this.currentTutorial.key)\n        .then(() => {\n          this.$emit(\"refreshList\");\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n  },\n  mounted() {\n    this.message = \"\";\n    this.currentTutorial = { ...this.tutorial }\n  },\n};\n</script>\n\n<style>\n.edit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/components"}]}